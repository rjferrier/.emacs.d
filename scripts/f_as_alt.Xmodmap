! Remap keys as follows:
! F, Alt  -> Alt
! Caps    -> F
! Alt Gr  -> Caps
! Windows -> Meta

clear Lock
clear Mod1
clear Mod3
clear Mod4

keycode 41 = Alt_L
keycode 66 = f
keycode 64 = Alt_L
keycode 108 = Caps_Lock
keycode 133 = Meta_L

add Lock = Caps_Lock
add Mod1 = Alt_L
add Mod3 = Meta_L
add Mod4 = Super_L Hyper_L
